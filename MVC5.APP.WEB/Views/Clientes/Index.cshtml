@model List<MVC5.APP.APPLICATION.Clientes.DTO.ClienteResponse>
@{
    ViewBag.Title = "Index";
}

<h2>View</h2>

<div class="row>

    <div class="col-10">Registrar Novo Cliente</div>
    <div class="col-2">
        @Html.ActionLink("Register New", "Cadastro", "Cliente", new { area = "" }, new { @class = "nav-link btn btn-info"})
    </div>

     <table class="table">
         <thead>
             <tr>
                 <th>Cpf</th>
                 <th>Nome</th>
                 <th>Endereco</th>
             </tr>
         </thead>
         <tbody>
             @foreach(var cliente in Model)
            {
                <tr>
                 <td>@cliente.Cpf</td>
                 <td>@cliente.Nome</td>
                <td>@cliente.Endereco</td>
                </tr>
            }
         </tbody>
     </table>

</div>